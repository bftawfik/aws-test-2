{
    "extends": ["next/core-web-vitals", "prettier"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": [
            "warn",
            {
                "endOfLine": "auto"
            }
        ],
        "no-console": "warn",
        "react/no-unescaped-entities": 0
    }
}
